package com.kh.array.practice.example;

public class LottoPractice {
	
	public static void main (String[] args) {
		//랜덤숫자 이용해서 
		//로또번호 6자리 중복없이 출력
		// 결과물은 [ 3, 45, 33, 21, 44, 22 ] 
		
		for(int i = 0 ; i<6; i++) {
			int random = (int)(Math.random() *45 + 1);
		}
		
		int[] num = new int[6];
		
		for(int i = 0; i<num.length; i++) {
			num[i] = random;
			
		}
		for(int i = 0; i<num.length; i++) {
			System.out.print(num[i] + " ");
		}
		
	}

}
