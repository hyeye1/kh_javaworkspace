package com.kh.array;

public class B_ArrayCopy {
	
	/*
	 * 배열 복사
	 * -얕은 복사 : 배열의 주소만을 복사
	 * -깊은 복사 : 동일한 새로운 배열을 하나 생성해서 실제 내부값들을 복사
	 * 
	 */
	
	public void method1() {
		
		int[] origin = {1,2,3,4,5}; //원본 배열
		
		System.out.println("원본 배열 출력");
		for(int i=0; i<origin.length; i++) {
			System.out.print(origin[i] + " ");
		}
	}

}
